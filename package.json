{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "5.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "gatsby": "2.28.2",
    "gatsby-plugin-manifest": "2.8.0",
    "gatsby-plugin-offline": "3.6.0",
    "gatsby-plugin-postcss": "3.3.0",
    "gatsby-plugin-react-helmet": "3.6.0",
    "gatsby-plugin-sharp": "2.10.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.15.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "8.2.1",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oddstronaut/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/oddstronaut/gatsby-starter-tailwind/issues"
  }
}
